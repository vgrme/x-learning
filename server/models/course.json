{
  "name": "Course",
  "plural": "courses",
  "mongodb": {
    "collection": "courses"
  },
  "base": "PersistedModel",
  "properties": {
    "title": {
      "type": "String"
    },
    "description": {
      "type": "String"
    },
    "date": {
      "type": "Date"
    },
    "cover": {
      "type": "String"
    },
    "language": {
      "type": "String"
    },
    "cost": {
      "type": "Number"
    },
    "publish": {
      "type": "Boolean"
    }
  },
  "relations": {
    "category": {
      "type": "belongsTo",
      "model": "Category",
      "foreignKey": "category_id"
    },
    "teacher": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "teacher_id"
    },
    "lectures": {
      "type": "hasMany",
      "model": "Lecture",
      "foreignKey": "course_id"
    },
    "webinars": {
      "type": "hasMany",
      "model": "Webinar",
      "foreignKey": "webinar_id"
    },
    "students": {
      "type": "hasAndBelongsToMany",
      "model": "Member",
      "through": "MemberCourse"
    },
    "review": {
      "type": "hasMany",
      "model": "Review",
      "foreignKey": "review_id"
    }
  },
  "acls": [],
  "methods": [],
  "validations": []
}